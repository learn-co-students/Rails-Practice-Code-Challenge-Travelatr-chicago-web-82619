<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <%= error %>
  <% end %>
<% end %>

<%= form_with model: @blogger do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "bio" %>
  <%= f.text_field :bio %>
  <%= f.label "age" %>
  <%= f.text_field :age %>
  <%= f.submit %>
<%end %>
